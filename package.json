{
  "name": "proto_slicer",
  "main": "src/electron-starter.js",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@types/three": "^0.134.0",
    "comlink": "^4.3.1",
    "custom-electron-titlebar": "^3.2.7",
    "electron-titlebar-windows": "^3.0.0",
    "faker": "^5.5.3",
    "fast-png": "^6.1.0",
    "fs": "^0.0.1-security",
    "jimp": "^0.16.1",
    "react-app-rewired": "^2.1.8",
    "sharp": "^0.29.3",
    "three-mesh-bvh": "^0.5.2",
    "ts-keycode-enum": "^1.0.6",
    "wait-on": "^6.0.0",
    "worker-loader": "^3.0.8"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron --dev .",
    "package": "electron-builder build --win --publish never"
  },
  "build": {
    "productName": "ProtoSlicer",
    "appId": "chall2.ProtoSlicer",
    "directories": {
      "buildResources": "build"
    },
    "extraMetadata": {
      "main": "src/electron-starter.js"
    },
    "files": [
      "./build/*",
      "./src/electron-starter.js",
      "./src/preload.js",
      "./src/store.js"
    ],
    "win": {
      "asar": true,
      "target": "nsis"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@react-three/drei": "^7.17.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/electron": "^1.6.10",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "bootstrap": "^5.1.1",
    "cross-env": "^7.0.3",
    "electron": "^15.1.0",
    "electron-builder": "^22.13.1",
    "electron-json-storage": "^4.5.0",
    "electron-packager": "^15.4.0",
    "mobx": "^6.3.5",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-rc.1",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "redux": "^4.1.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "three": "^0.133.1",
    "typescript": "^4.4.4",
    "web-vitals": "^1.0.1"
  },
  "homepage": "./",
  "description": "slicer",
  "author": "chall2 || chall4",
  "config-overrides-path": "react-app-rewired.config.js"
}
